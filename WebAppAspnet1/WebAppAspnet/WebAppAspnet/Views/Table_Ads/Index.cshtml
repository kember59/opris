@model IEnumerable<WebAppAspnet.Models.Table_Ads>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.adTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adConfirm)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adDatePublic)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.id_Rubric)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.adTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.adConfirm)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.adDatePublic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.adText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.id_Rubric)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_Ad }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_Ad }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_Ad })
        </td>
    </tr>
}

</table>
